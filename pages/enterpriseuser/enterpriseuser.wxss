/* pages/enterpriseuser/enterpriseuser.wxss */
@import'../balancelist/balancelist.wxss';
.top-banner{
  position: relative;
  height: 100%;
  top: 0;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content: center;
}
.weui_cell_bd_p{
  color: #ee86a1;
}
.item-jifen{
  padding:20rpx 30rpx;
}
.weui_cell_hd{
  display: inline-block;
  width: 80rpx; 
  height: 80rpx; 
   margin-right: 10rpx; 
}
.weui_cell_hd image{
  width: 80rpx;
  height: 80rpx;
  vertical-align: -4rpx;
}
.weui_cell_bd{
  width: 100%;
  display: inline-block;
   height: auto; 
}
.weui_cell_bd_p{
  font-size: 30rpx;
  color: #1a3064;
  padding-bottom: 10rpx;
}
.weui_cell_bd_dec{
	 height: auto; 
	font-size: 26rpx;
	color: #999;
}
.weui_cell{
  border-bottom: 0;
}
.line{
  width: 100%;
  height: 20rpx;
  background-color: #f5f5f5;
}
.liucheg{
  /* width: 100%; */
  height: 111rpx;
  margin: 30rpx;
}
.liucheg image{
  width: 100%;
  display: block;
  height: 100%;
}
.btn{
  background-color: #1a3064;
  color: #fff;
}
.form-sub{
  margin-bottom: 30rpx;
}
.uploader{
  position: absolute;
  right: 30rpx;
  width: 30rpx;
  height: 30rpx;
}
.uploader image{
  width: 100%;
  height: 100%;
}
.cell{
  display: flex;
  /* text-align: center; */
  
}
.image-cell{
  width: 30%;
  height: 150rpx;
  position: relative;
  margin-right: 20rpx

}
.image-cell image{
  width: 100%;
  height: 100%;
}
.delete-btn{
 width:40rpx;
height:40rpx;
position:absolute;
top:-10rpx;
right:-10rpx;
}
.imagess {
width:100%;
height:100%;
}
.userlist {
