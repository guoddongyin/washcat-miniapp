.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  overflow: hidden;  
}
.drawer_screen image{
  width: 100%;
  height: 100%
}
