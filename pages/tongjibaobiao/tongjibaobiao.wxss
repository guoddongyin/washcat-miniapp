.navbtns{
    display: flex;
    height: 100rpx;
    flex-wrap: nowrap;
    flex-direction: row;
    align-items: center;
    background-color: #ffffff;
    border-bottom: 1rpx solid #f2f2f2;
}

.navbtns .navitem{
    flex: 1;
    text-align: center;
    color: #777;
    font-size: 26rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.navbtns .navitem image{
    display: inline-block;
    width: 25rpx;
    height: 15rpx;
    margin-left: 6rpx;
    vertical-align: middle;
}

.navbtns .line{
width: 3rpx;
background-color: #aaaaaa;
height: 45rpx;
flex-shrink: 0;
}

.wrap{
  position:fixed;
  height:100%;
  padding-top:30rpx;
  width:100%;
}

.wrap .spinner{
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .3);
    z-index: 1;
    top: 0;
    left: 0;
}

.wrap .spinner .items{
    background-color: #fff;
    /* padding-left: 20rpx; */
}

.wrap .spinner .items .item{
   padding-left: 25rpx;
border-bottom: 1rpx solid #f2f2f2;
height: 90rpx;
line-height: 90rpx;
color: #777;
font-size: 30rpx;
text-align:center;

}

