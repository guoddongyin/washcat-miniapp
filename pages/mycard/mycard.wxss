/* pages/mycard/mycard.wxss */
.content{
  padding-bottom: 100rpx;
}
image{
  width:350rpx;
  height:280rpx;
  background-size: 100% 100%;
}
.zhu{
  font-size: 36rpx;
  color: #333;
  text-align: center;
  margin-top: 60rpx;
}
.viewclass{
  position: fixed;
  height: 80%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content: center;
  }
.drawer{
width:510rpx;
height:300rpx;
overflow:hidden;
position:fixed;
top:210rpx;
right:110rpx;
  z-index: 1001;    
  margin: -200rpx 0rpx 0rpx 110rpx;   
}
.drawer image{
  width: 100%;
  height: 100%;
}
.my-card{
  width: 85%;
  height: 230rpx;
  margin: auto;
  background-image:url('data:image/png;base64,');
  background-size: 100% 100%;
}
.active{
  width: 85%;
  height: 230rpx;
  margin: auto;
  background-image:url(data:image/png;base64,);
  background-size: 100% 100%;
}
.cell-hd{
  width: 60%;
  display: inline-block;
  height: 100rpx;
  margin-top: 95rpx;
}
.cell-ft{
  width: 40%;
  display: inline-block;
  text-align: right
}
.cell-hd-text{
  padding-left: 50rpx;
  color: #e35274;
  font-size: 40rpx;

}
.cell-hd-dec{
  padding-left: 50rpx;
  padding-top: 20rpx;
  color: #e35274;
  font-size: 25rpx;
  display: block;
}
.cell-ft image{
  width: 150rpx;
  height: 40rpx;
  padding-right: 50rpx;
  /* padding-top: 20rpx; */
}
.cell-hd-num{
  color: #e35274;
   padding-right: 50rpx; 
   font-size: 80rpx;
}
.chi{
  font-size: 25rpx;
}
.cell-button{
  width: 100%;
  height: 100rpx;
  background: #ddd;
  position: fixed;
  bottom: 0
}
.btn-buttom{
  position: fixed;
  bottom: 0;
  border-top:1rpx solid #ededed;
}
.btn1{
  background-color: #fff;
  color: #1a3064;
}
.btn2{
  background-color: #1a3064;
  color: #fff;
}
 
/*弹窗  */

/*mask*/  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.5;  
  overflow: hidden;  
}  
  
/*content*/  
.drawer_box {  
  width: 540rpx; 
  height: 300rpx; 
  overflow: hidden;  
  position: fixed;  
  top: 50%;  
  left: 0;  
  z-index: 1001;  
  background: white;  
  margin: -200rpx 0rpx 0rpx 110rpx;  
  border-radius: 20rpx;  
}  
  
.drawer_title{  
  padding:30rpx;  
  font: 31rpx "microsoft yahei";  
  text-align: center;  
  color: #999999
}  
.drawer_content {  
  height: 100rpx;  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
  text-align: center
}  

.top{  
    padding-top:8px;  
}  

.input_base {  
    padding-left: 10rpx;  
    margin-right: 50rpx;  
}  
.input_h30{  
    height: 30px;  
    line-height: 30px;  
}  

.btn_ok{  
  padding: 10px;  
  font: 20px "microsoft yahei";  
  text-align: center;  
  border-top: 1px solid #E8E8EA;  
  color: #3CC51F;  
}  
  
