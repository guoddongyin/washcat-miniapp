.colorred{
  color: #ff0000
}

.grid { display: -webkit-box; display: box; }  
.col-0 {-webkit-box-flex:0;box-flex:0;}  
.col-1 {-webkit-box-flex:1;box-flex:1;}  
.fl { float: left;}  
.fr { float: right;} 


.ditxt{
  font-size: 24rpx;
	color: #999999;
}
.next_bottom{
  width: 60%;
  background-color: #1a3064;
  color: #fff;
  border-radius: 50rpx;
}

.number-sub-btn {
width:50rpx;
height:50rpx;
vertical-align:middle;
padding:20rpx 0;
}

.blank-distance{
    width:80rpx;
    height:30rpx;
    display: inline-block;
    position: relative;
    vertical-align: middle;
}

.number-add-btn{
    width:50rpx;
    height:50rpx;
    vertical-align:middle;
    padding:20rpx 0;
}

.button-group{
    position:fixed;
    width: 100%;
    bottom:0;
    left:0;
    display: flex;
    height:99rpx;
}
.number-selector{
    flex:3;
    margin-left:30rpx;
}

.number-selector{
    width:200rpx;
    margin: auto;
    position: relative;
}

.number-display{
    position: absolute;
    height:20rpx;
    line-height: 20rpx;
    top:0;
    bottom:0;
    left:0;
    right:0;
    margin:auto;
    font-size: 16px;
    color: #888888;
    text-align: center;
}