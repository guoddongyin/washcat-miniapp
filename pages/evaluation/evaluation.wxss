/* pages/evaluation/evaluation.wxss */
@import'../mycard/mycard.wxss';
.cell-hd{
  width: 60%;
  display: inline-block;
  height: 100rpx;
  margin-top: 140rpx;
}
.star{
    margin-top: 30rpx;
    margin-bottom: 50rpx;
}
.star-description{
    font-size: 28rpx;
    color: #333333;
    margin-left: 30rpx;
}
.star-content {
    /* position: absolute;
    right: 0px; */
    margin-right: 40rpx;
    width: 400rpx;
    margin: auto;
}
.uploader{
  width: 50rpx;
  height: 50rpx;
  margin-left: 30rpx;
}
.uploader image {
  width: 100%;
  height: 100%;
} 
.star .star-content .starNo{
    z-index: 2;
    background-image: url(data:image/png;base64,);
    width: 80rpx;
    height: 65rpx;
    background-size: 65rpx 65rpx;
    background-position: center center;
    background-repeat: no-repeat;
}

.starsHas{
    z-index: 1;
    width: 80rpx;
    height: 65rpx;
    background-image: url(data:image/png;base64,);
    background-size: 65rpx 65rpx;
    background-position: center center;
    background-repeat: no-repeat;
}
.pinglun{
  font-size: 30rpx;
	/* color: #999; */
  /* margin: 0 30rpx; */
  width: 90%;
  margin: auto
}
.pinglun textarea{
  width: 100%
}
.pinglun-ima image{
  width: 100%;
  height: 100%;
border-radius: 10rpx
}
.pinglun-ima{
  height: 170rpx;
  position: relative;
  margin-right: 20rpx;
  /* border-radius: 10rpx */
}
 .pinglun-row{
  display: flex;
  margin: 20rpx;
} 
.btn{
  background-color: #97d5e6;
  color: #1a3064;
  position: fixed;
  bottom: 0;
  width: 100%;
}
.delete-btn{
 width:40rpx;
height:40rpx;
position:absolute;
top:-10rpx;
right:-10rpx;
}