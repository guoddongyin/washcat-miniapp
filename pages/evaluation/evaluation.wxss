@import'../mycard/mycard.wxss';
.star{
    margin-top: 30rpx;
    margin-bottom: 50rpx;
}
.star-description{
    font-size: 28rpx;
    color: #333333;
    margin-left: 30rpx;
}
.star-content {
    /* position: absolute;
    right: 0px; */
    margin-right: 40rpx;
    width: 400rpx;
    margin: auto;
}
.star .star-content .starNo{
    z-index: 2;
    background-image: url(data:image/png;base64,);
    width: 77rpx;
    height: 60rpx;
    background-size: 60rpx 60rpx;
    background-position: center center;
    background-repeat: no-repeat;
}

.starsHas{
    z-index: 1;
    width: 77rpx;
    height: 60rpx;
    background-image: url(data:image/png;base64,);
    background-size: 60rpx 60rpx;
    background-position: center center;
    background-repeat: no-repeat;
}
