/* pages/buycard/buycard.wxss */
@import'../mycard/mycard.wxss';
.txt3 {
width:100%;
}
.weui_cell_hd{
  font-size: 30rpx;
  color: #666;
  padding: 15rpx 0
}
.with_arrow:after {
    content: " ";
    display: inline-block;
    height: 16rpx;
    width: 16rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #b2b2b2;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    top: -4rpx;
    position: absolute;
    top: 50%;
    margin-top: -8rpx;
    right: 0rpx;
    border-radius: 2rpx;
}
.cell-hd-num {
color:#fff;
padding-right:50rpx;
font-size:80rpx;
}
.cell-hd-dec{
  color: #fff;
}
.cell-hd-text{
  color: #fff
}
.my-card{
  width: 85%;
   background-image:url('data:image/png;base64,')
}
.item-jifen{
  display: block;
  font-size:36rpx;
  padding: 30rpx;
  text-align: left;
}
.row-1 image{
  width: 40rpx;
  height: 40rpx;
  display: block;
  padding: 30rpx ;
}
.btn{
  width: 100%;
  position: fixed;
  bottom: 0;
  background-color: #e35274;
  color: #fff;
}
.money-cell{
  width: 85%;
  margin: auto;
}
.money{
  font-size: 60rpx;
	letter-spacing: 3rpx;
	color: #e35274;
}
.zhang{
	font-size: 26rpx;
	letter-spacing: 1rpx;
	color: #666666;
}
.money-cell image{
  width: 20rpx;
  height: 20rpx;
  padding-right: 10rpx;
}